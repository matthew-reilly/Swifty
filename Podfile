project 'MVP.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'MVP' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MVP

  pod 'Alamofire', '~> 3.4.0'
  pod 'WWAppearance', :git => 'git@github.com:weddingwire/appearance-ios.git', :branch => 'dev'
  pod 'WWRequestKit', :git => 'git@github.com:weddingwire/WWRequestKit.git', :branch => 'master'
  pod 'SnapKit', '0.18.0'

  target 'MVPTests' do
    inherit! :search_paths


    pod 'Quick', '~> 0.8.0'
    pod 'Nimble', '~> 3.2.0'

  end

end
